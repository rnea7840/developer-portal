  "jest": {
    "projects": [
      "e2e.jest.config.js",
      "unit.jest.config.js",
      "visual.jest.config.js"
    ]
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "forceAllTransforms": true,
          "loose": true,
          "corejs": 2,
          "modules": false,
          "exclude": [
            "transform-typeof-symbol"
          ],
          "targets": {
            "ie": "11",
            "chrome": "54",
            "firefox": "46",
            "node": "current"
          }
        }
      ],
      [
        "react-app",
        {
          "flow": false,
          "typescript": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ]
