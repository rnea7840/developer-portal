  "name": "developer-portal",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://developer.va.gov/",
  "proxy": "http://localhost:8080/",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "lint": "./node_modules/.bin/eslint --ext .ts,.tsx src",
    "test": "echo You should run test:unit, test:e2e, test:accessibility, or test:visual",
    "test:unit": "node scripts/test.js --projects=unit.jest.config.js",
    "test:unit:ci": "node scripts/test.js --projects=unit.jest.config.js --ci --testResultsProcessor=\"./node_modules/jest-junit-reporter\" --runInBand --coverage",
    "test:cypress": "CYPRESS_SINGLE_SPEC=true ./node_modules/.bin/cypress open",
    "test:e2e": "./node_modules/.bin/cypress run",
    "test:e2e:ci": "./scripts/cypress_test.sh run",
    "test:monitoring": "./node_modules/.bin/cypress run -s ./cypress/e2e/App.cy.js",
    "test:accessibility": "CYPRESS_SINGLE_SPEC=true ./node_modules/.bin/cypress run -s ./cypress/e2e/accessibility.cy.js",
    "test:accessibility:ci": "CYPRESS_SINGLE_SPEC=true ./scripts/cypress_test.sh run -s ./cypress/e2e/accessibility.cy.js",
    "test:visual": "CYPRESS_SINGLE_SPEC=true ./node_modules/.bin/cypress run --reporter cypress-image-snapshot/reporter -s ./cypress/e2e/visual.cy.js",
    "test:visual:update": "CYPRESS_SINGLE_SPEC=true ./node_modules/.bin/cypress run --env updateSnapshots=true -s ./cypress/e2e/visual.cy.js",
    "test:coverage": "node scripts/test.js --coverage "
  },
